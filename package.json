{
  "name": "sstpoc-cdk",
  "version": "0.1.0",
  "bin": {
    "sstpoc-cdk": "bin/sstpoc-cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk --profile=sstpoc",
    "cdk:deploy": "cdk deploy --all --profile=sstpoc",
    "cdk:destroy": "cdk destroy --all --profile=sstpoc",
    "cdk:diff": "cdk diff --profile=sstpoc",
    "cdk:synth": "cdk synth --profile=sstpoc"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "18.14.6",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "aws-cdk": "2.74.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.74.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
